{
  "name": "superintendent",
  "version": "7.0.0",
  "description": "Write SQLs on CSVs",
  "license": "UNLICENSED",
  "main": "dist/prod/main.js",
  "scripts": {
    "build:watch": "webpack --watch --config webpack.dev.js",
    "start": "electron --app=dist/dev/main.js",
    "test": "jest --useStderr",
    "wdio": "ENABLE_WDIO=yes wdio run ./wdio.conf.ts",
    "lint:fix": "eslint --ext ts,tsx --fix src test"
  },
  "build": {
    "appId": "tanin.superintendent",
    "directories": {
      "buildResources": "electron-builder/build",
      "output": "electron-builder/out"
    },
    "appImage": {
      "mimeTypes": [
        "text/csv",
        "text/tab-separated-values"
      ]
    },
    "fileAssociations": [
      {
        "ext": "csv",
        "role": "Viewer",
        "name": "Comma-separated values"
      },
      {
        "ext": "tsv",
        "role": "Viewer",
        "name": "Tab-separated values"
      },
      {
        "ext": "psv",
        "role": "Viewer",
        "name": "Pipe-separated values"
      },
      {
        "ext": "super",
        "role": "Viewer",
        "name": "Superintendent workflow"
      }
    ],
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64"
        ]
      },
      "category": "public.app-category.finance",
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "electron-builder/build/entitlements.mac.plist",
      "entitlementsInherit": "electron-builder/build/entitlements.mac.plist",
      "notarize": false
    },
    "files": [
      "dist/prod",
      "package.json",
      "!.git{/*}",
      "!.idea{/*}",
      "!out{/*}",
      "!src{/*}"
    ],
    "asarUnpack": [
      "dist/prod",
      "node_modules"
    ],
    "afterSign": "scripts/afterSign.js",
    "afterAllArtifactBuild": "scripts/afterAllArtifactBuild.js",
    "npmRebuild": false
  },
  "author": "Tanin Na Nakorn",
  "resolutions": {
    "@types/react": "17.0.85",
    "@types/react-dom": "17.0.11"
  },
  "devDependencies": {
    "@electron/notarize": "2.3.0",
    "@eslint/js": "^9.24.0",
    "@types/archiver": "^6.0.2",
    "@types/chart.js": "^2.9.32",
    "@types/codemirror": "^5.60.0",
    "@types/jest": "29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/randomcolor": "^0.5.9",
    "@types/react": "17.0.85",
    "@types/react-dom": "17.0.11",
    "@types/react-window": "1.8.8",
    "@wdio/cli": "9.12.4",
    "@wdio/jasmine-framework": "9.12.4",
    "@wdio/local-runner": "9.12.4",
    "@wdio/spec-reporter": "9.12.3",
    "css-loader": "^5.0.1",
    "electron": "35.1.4",
    "electron-builder": "26.0.12",
    "eslint": "9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "file-loader": "^6.2.0",
    "globals": "^16.0.0",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "node-abi": "3.52.0",
    "node-gyp": "^10.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.86.3",
    "sass-loader": "16.0.5",
    "style-loader": "^2.0.0",
    "threads-plugin": "^1.4.0",
    "ts-jest": "29.3.1",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "*",
    "typescript-eslint": "^8.29.0",
    "wdio-electron-service": "8.1.0",
    "web-worker": "^1.2.0",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.7.3",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@aptabase/electron": "^0.3.1",
    "@floating-ui/react": "^0.26.9",
    "@sentry/electron": "^4.24.0",
    "@types/unzipper": "^0.10.9",
    "archiver": "^7.0.1",
    "chart.js": "4.4.2",
    "codemirror": "^5.61.1",
    "csv-parse": "4.15.4",
    "duckdb-async": "1.2.1",
    "electron-default-menu": "^1.0.2",
    "electron-store": "^8.0.0",
    "jszip": "^3.10.1",
    "luxon": "^3.4.4",
    "nan": "^2.14.0",
    "react-modal": "^3.14.3",
    "react-virtualized-auto-sizer": "1.0.26",
    "react-window": "git+ssh://git@github.com:tanin47/react-window.git#c8b14c840cd6a6b4672600a081d9614afc16fa5d",
    "react-window-infinite-loader": "1.0.10",
    "sql-formatter": "15.3.0",
    "sweetalert2": "11.10.7",
    "threads": "^1.6.5",
    "tiny-worker": "^2.3.0"
  },
  "packageManager": "yarn@4.8.1",
  "repository": "git@github.com:tanin47/superintendent-app.git",
  "private": true
}
