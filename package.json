{
  "name": "superintendent",
  "version": "4.0.0-rc6",
  "description": "Write SQLs on CSVs",
  "license": "UNLICENSED",
  "main": "dist/prod/main.js",
  "scripts": {
    "build:watch": "webpack --watch --config webpack.dev.js",
    "start": "electron dist/dev/main.js",
    "test": "jest --useStderr",
    "wdio": "wdio run ./wdio.conf.ts"
  },
  "build": {
    "appId": "tanin.superintendent",
    "directories": {
      "buildResources": "electron-builder/build",
      "output": "electron-builder/out"
    },
    "appImage": {
      "mimeTypes": [
        "application/x-sqlite3",
        "application/vnd.sqlite3",
        "text/csv",
        "text/tab-separated-values"
      ]
    },
    "fileAssociations": [
      {
        "ext": "csv",
        "role": "Viewer",
        "name": "Comma-separated values"
      },
      {
        "ext": "tsv",
        "role": "Viewer",
        "name": "Tab-separated values"
      },
      {
        "ext": "psv",
        "role": "Viewer",
        "name": "Pipe-separated values"
      },
      {
        "ext": "super",
        "role": "Viewer",
        "name": "Superintendent workflow"
      }
    ],
    "mac": {
      "category": "public.app-category.finance",
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "electron-builder/build/entitlements.mac.plist",
      "entitlementsInherit": "electron-builder/build/entitlements.mac.plist"
    },
    "files": [
      "dist/prod",
      "package.json",
      "!.git{/*}",
      "!.idea{/*}",
      "!out{/*}",
      "!src{/*}"
    ],
    "extraResources": [
      "./deps/csv_writer/csv_writer.dylib",
      "./deps/csv/csv.dylib",
      "./deps/ext/ext.dylib",
      "./deps/csv_writer/csv_writer.dll",
      "./deps/csv/csv.dll",
      "./deps/ext/ext.dll",
      "./deps/csv_writer/csv_writer.so",
      "./deps/csv/csv.so",
      "./deps/ext/ext.so"
    ],
    "asarUnpack": [
      "dist/prod",
      "node_modules"
    ]
  },
  "author": "Tanin Na Nakorn",
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.1",
    "@types/chart.js": "^2.9.32",
    "@types/codemirror": "^5.60.0",
    "@types/electron": "^1.6.10",
    "@types/jest": "^29.5.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@wdio/cli": "^8.26.0",
    "@wdio/jasmine-framework": "^8.26.0",
    "@wdio/local-runner": "^8.26.0",
    "@wdio/spec-reporter": "^8.24.12",
    "css-loader": "^5.0.1",
    "electron": "^19.0.7",
    "electron-builder": "23.0.3",
    "electron-notarize": "1.2.1",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^27.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "threads-plugin": "^1.4.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^8.0.11",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "wdio-electron-service": "^6.0.0",
    "web-worker": "^1.2.0",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.7.3",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@tippyjs/react": "^4.2.5",
    "better-sqlite3": "tanin47/better-sqlite3#54c74c5be05988766826f27ee124d2c6fe4d7730",
    "chart.js": "^3.3.2",
    "codemirror": "^5.61.1",
    "csv-parse": "4.15.4",
    "electron-default-menu": "^1.0.2",
    "electron-store": "^8.0.0",
    "nan": "^2.14.0",
    "randomcolor": "^0.6.2",
    "react-modal": "^3.14.3",
    "react-virtualized-auto-sizer": "^1.0.5",
    "react-window": "tanin47/react-window#4bed2c46bc9342a527a40c1495d9037e0def4fd7",
    "react-window-infinite-loader": "^1.0.7",
    "sql-formatter": "^4.0.2",
    "sweetalert2": "^11.0.18",
    "threads": "^1.6.5",
    "tiny-worker": "^2.3.0",
    "tippy.js": "^6.3.1"
  }
}
